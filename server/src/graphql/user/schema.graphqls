type User {
  id: ID
  full_name: String
  email: String
  created_at: String
}

type Query {
  currentUser: User
}

type Mutation {
  register(email: String!, full_name: String!, password: String!): User
  login(email: String!, password: String!): User
  resetPassword(email: String!): String
  changePasswordAfterReset(
    email: String!
    token: String
    password: String
  ): String
}
